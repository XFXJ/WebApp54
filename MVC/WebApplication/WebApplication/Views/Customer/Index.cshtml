@model WebApplication.ViewModels.InformationListViewModel
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <h2>@Model.UserName,@Model.Greeting</h2>
    <div> 
        <ul>
            @foreach (var item in Model.Informat)
            {
                <li>@item.InformationName,@item.Salary，@item.InformationGrade</li>
            }
        </ul>
    </div>
</body>
</html>
